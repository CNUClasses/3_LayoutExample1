<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.layoutexample">

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:exported="true"
            android:name=".LayoutActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FrameLayoutActivity"
            android:label="Table Layout"></activity>
        <activity
            android:name=".TableActivity"
            android:label="Table Layout"></activity>
        <activity
            android:name=".WeightGravityActivity"
            android:label="@string/title_activity_table"></activity>
        <activity
            android:name=".LinearActivity"
            android:label="@string/title_activity_linear"></activity>
        <activity
            android:name=".RelativeActivity"
            android:label="@string/title_activity_relative"></activity>
        <activity
            android:name=".GridLayoutActivity"
            android:label="@string/title_activity_grid_layout"></activity>
    </application>

</manifest>